{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "module": "es2022",
    "moduleResolution": "node",
    "outDir": "./.tsbuild",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
